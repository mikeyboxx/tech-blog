<div class="posts">
  {{#each posts}}
  <div class="card">
    <header class="card-header">
      <div class="card-header-title">
        <a class="blog-title-link" href="/post/{{id}}">
          {{title}}
        </a>
        <p>
          Posted by {{user.dataValues.username}} on {{createdAt}}
        </p> 
      </div>
    </header>
    <div class="card-content">
      <div class="content">
        {{content}}
      </div>
    </div>
  </div>
  {{/each}}
</div>